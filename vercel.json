{
  "crons": [
    {
      "path": "/api/cron/daily-reminders",
      "schedule": "0 * * * *"
    }
  ],
  "functions": {
    "app/api/reservations/route.ts": {
      "maxDuration": 60
    },
    "app/api/cron/daily-reminders/route.ts": {
      "maxDuration": 300
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        }
      ]
    }
  ]
}